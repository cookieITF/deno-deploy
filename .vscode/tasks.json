{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "deno",
      "command": "lint",
      "presentation": {
        "echo": false,
        "reveal": "silent",
        "focus": false,
        "panel": "shared",
        "showReuseMessage": true,
        "clear": false
      },
      "problemMatcher": [
        "$deno-lint"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "label": "deno: lint"
    }
  ]
}
